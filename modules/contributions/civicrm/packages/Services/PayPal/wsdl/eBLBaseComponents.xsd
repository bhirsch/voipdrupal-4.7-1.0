	<complexType name="CustomSecurityHeaderType">
		<annotation>
			<documentation>
      			    Custom Securiy Header.
      			</documentation>
		</annotation>
		<sequence>
			<element name="eBayAuthToken" type="xs:string" minOccurs="0"/>
